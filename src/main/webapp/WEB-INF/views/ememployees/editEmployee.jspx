    <script type="text/javascript">
        var Label = {};
        var Button = {};
        var Message = {};

        Label.LABEL_POINT = '${LABEL_POINT}';
        Label.LABEL_LABEL_MODULE_MANAGER = '${LABEL_MODULE_MANAGER}';

        Button.BUTTON_OK = '${BUTTON_OK}';

        Message.MSG_DATA_NOT_FOUND = '${MSG_DATA_NOT_FOUND}';
        Message.MSG_PLEASE_ENTER_YOUR_EMID_AGAIN = '${MSG_PLEASE_ENTER_YOUR_EMID_AGAIN}';
        Message.MSG_SUBSCRIBE_SUCCESSFULLY = '${MSG_SUBSCRIBE_SUCCESSFULLY}';
        Message.MSG_PLESE_ENTER_YOUR_PASSWORD_AT_LAST_8_CHAR = '${MSG_PLESE_ENTER_YOUR_PASSWORD_AT_LAST_8_CHAR}';
        Message.MSG_PLESE_SELECT_TEAM = '${MSG_PLESE_SELECT_TEAM}';
        Message.MSG_PLEASE_SELECT_POSITION = '${MSG_PLEASE_SELECT_POSITION}';
        Message.MSG_PLEASE_CONFIRM_YOUR_PASSWORD_AGAIN = '${MSG_PLEASE_CONFIRM_YOUR_PASSWORD_AGAIN}';
        Message.MSG_PLEASE_SELECT_THE_DATA_TO_BE_DELETED = '${MSG_PLEASE_SELECT_THE_DATA_TO_BE_DELETED}';
        Message.MSG_DO_YOU_WANT_TO_REMOVE_THE_SELECTED_ITEMS = '${MSG_DO_YOU_WANT_TO_REMOVE_THE_SELECTED_ITEMS}';
        Message.MSG_DELETE_SUCCESS = '${MSG_DELETE_SUCCESS}';
        Message.MSG_LIST = '${MSG_LIST}';
        Message.MSG_DELETE_FAILED = '${MSG_DELETE_FAILED}';
        Message.MSG_DATA_ALL_IN_USED = '${MSG_DATA_ALL_IN_USED}';
        Message.MSG_INUSE = '${MSG_INUSE}';

    </script>



    <div class="row">
        <div class="col-xs-12">
            <div class="panel panel-primary">
                <div class="panel-heading">${LABEL_EDIT_EMPLOYEE}</div>
                <div class="panel-body">
                    <div class="col-sm-12">
                        <form class="form-horizontal">
                            <div class="form-group col-sm-6">
                                <label class="control-label col-sm-3">${LABEL_EMPLOYEE_CODE}</label>
                                <div class="col-sm-8">
                                    <input type="text" class="form-control" id="empCode" placeholder="${LABEL_EMPLOYEE_CODE}"/>
                                </div>
                            </div>
                            <div class="form-group col-sm-6">
                            <label class="control-label col-sm-3">${LABEL_NAME}</label>
                            <div class="col-sm-8">
                                <input type="text" class="form-control" id="empFirstName" placeholder="${LABEL_NAME}"/>
                            </div>
                        </div>
                            <div class="form-group col-sm-6">
                                <label class="control-label col-sm-3">${LABEL_USERNAME}</label>
                                <div class="col-sm-8">
                                    <input type="text" class="form-control" id="userName" placeholder="${LABEL_USERNAME}"/>
                                </div>
                            </div>
                            <div class="form-group col-sm-6">
                                <label class="control-label col-sm-3">${LABEL_NICKNAME}</label>
                                <div class="col-sm-8">
                                    <input type="text" class="form-control" id="empNickName" placeholder="${LABEL_NICKNAME}"/>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="col-sm-12">
                        <form class="form-horizontal">
                            <div class="form-group col-sm-6">
                                <label class="control-label col-sm-3">${LABEL_TEAM}</label>
                                <div class="col-sm-8">
                                    <select class="form-control" id="emTeam" data-content="${MSG_PLESE_FALL}"><jsp:text/>
                                        <option value="0"></option>
                                        <c:forEach var="emTeam" items="${listEmteam}">
                                            <option value="${emTeam.id}${LABEL_DELIMETER}${emTeam.version}" >
                                                ${emTeam.teamName}
                                            </option>
                                        </c:forEach>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group col-sm-6">
                                <label class="control-label col-sm-3">${LABEL_POSTION}</label>
                                <div class="col-sm-8">
                                    <select class="form-control" id ="emPosition" data-content="${MSG_PLESE_FALL}" ><jsp:text/>
                                        <option value="0" ></option>
                                        <c:forEach var="emposition" items="${listEmpost}">
                                            <option value="${emposition.id}${LABEL_DELIMETER}${emposition.version}">
                                                ${emposition.positionName}
                                            </option>
                                        </c:forEach>

                                    </select>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="col-sm-8">
                        <div class="col-sm-offset-8 col-sm-2">
                            <button type="button" class="btn btn-primary" id="btnSearch">${BONTON_SEARCH}</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12">
            <div class="card">
                <div class="card-body">
                    <button type="button" class="btn btn-primary" data-toggle ="modal" id="Addemployee" data-backdrop="static">${BONTON_ADD}
                    </button>
                    <button style="margin-left: 5px" id="btnDelete" type="button" class="btn btn-danger">${BONTON_DELECT}</button>

                    <div class="form-group">
                        <div class="table-stripted table-responsive">
                            <table id="Table" class="table table-bordered table-stripted ">
                                <tdead>
                                    <tr>
                                        <th class="text-center"  width="5%"><input id="checkboxAll" type="checkbox" name="checkdDelete" /></th>
                                        <th class="text-center"  width="5%">${LABEL_EDIT}</th>
                                        <th class="text-center"  width="5%">${LABEL_EMPLOYEE_CODE}</th>
                                        <th class="text-center"  width="10%">${LABEL_NAME}</th>
                                        <th class="text-center"  width="10%">${LABEL_LAST_NAME}</th>
                                        <th class="text-center"  width="5%">${LABEL_NICKNAME}</th>
                                        <th class="text-center"  width="15%">${LABEL_POSTION}</th>
                                        <th class="text-center"  width="10%">${LABEL_TEAM}</th>
                                        <th class="text-center"  width="5%">สิทธิ์การใช้งาน</th>
                                        <!--<th class="text-center"  width="10%">${LABEL_USERNAME}</th>-->
                                        <!--<th class="text-center"  width="10%">${LABEL_PASSWORD}</th>-->
                                    </tr>
                                </tdead>
                                <tbody id="ResualtSearch" style="cursor:pointer"></tbody>
                            </table>
                        </div>
                        <div class="form-horizontal">
                            <div class="form-group">
                                <component:pagging id="paggingSimple"/>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script>
        var contextPath = '${pageContext.request.contextPath}';
    </script>

